                    <h3 class="title-md text-theme hr-left">Plus de detail</h3>
                    <p class="text-theme lead">Description :{{ description }}</p>
                    <h3 class="title-md text-theme">Heure :{{ horaire }}</h3>
                    <ul class="list-unstyled list-md text-theme">
                        <li><i class="fa fa-map-marker fa-box"></i>{{ lieux  }}</li>
                        <li><i class="fa fa-phone fa-box"></i>{{ n_tel }}</li>
                        <li><i class="fa fa-envelope fa-box"></i>AllForKids@gmail.com</li>
                    </ul>
                    <h3 class="title-md text-theme">Date disponibilit√©</h3>
                    <ul class="list-unstyled list-md text-theme">
                        <li><i class="fa fa-clock-o fa-box"></i>{{ date_dispo |join ('-')}}</li>
                    </ul>
                </div>
                {{ facebookButton()}}

                {{ twitterButton()}}

                {{ googleplusButton()}}

                {{ linkedinButton()}}
